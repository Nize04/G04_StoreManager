{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DOTNET_SYSTEM_GLOBALIZATION_INVARIANT": "false",
  "Jwt": {
    "Key": "Your_Secret_key_here", // Replace this with an actual environment variable
    "Issuer": "https://localhost:7054",
    "Audience": "https://localhost:7054"
  },
  "ConnectionStrings": {
    "DefaultConnection": "your_connectionstring_here" // Replace this with an actual environment variable
  },
  "AzureStorageSettings": {
    "ConnectionString": "your-new-storage-key-here", // Replace this with an actual environment variable
    "ContainerName": "your_containername_here"
  },
  "EmailSettings": {
    "SendGridApiKey": "UrSendeGridApiKey", // Replace this with an actual environment variable
    "FromEmail": "Sender Email"
  }
}